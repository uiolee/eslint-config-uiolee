{
  "name": "eslint-config-uiolee",
  "version": "0.1.0",
  "description": "eslint flat-config",
  "keywords": [
    "eslint",
    "config",
    "flat-config",
    "module"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uiolee/eslint-config-uiolee.git"
  },
  "license": "MPL-2.0",
  "author": "uiolee (https://github.com/uiolee)",
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./js": "./lib/config/js.js",
    "./ts": "./lib/config/ts.js",
    "./jest": "./lib/config/jest.js",
    "./package.json": "./package.json"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "pnpm esb",
    "esb": "rm -rf lib && node scripts/esb.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:type": "tsc --noEmit",
    "sort": "sort-package-json",
    "tsb": "tsc -b -v",
    "tsbf": "tsc -b -v -f",
    "tsbw": "tsc -b -v -w"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-n": "^17.2.1",
    "eslint-plugin-prettier": "^5.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "esbuild": "^0.20.2",
    "esbuild-register": "^3.5.0",
    "eslint": "^9.1.0",
    "sort-package-json": "^2.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^9.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-n": "^17.2.1",
    "eslint-plugin-prettier": "^5.1.3"
  },
  "peerDependenciesMeta": {
    "*": {
      "optional": false
    }
  },
  "packageManager": "pnpm@8.15.7+sha256.50783dd0fa303852de2dd1557cd4b9f07cb5b018154a6e76d0f40635d6cee019",
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
