{
  "name": "eslint-config-uiolee",
  "version": "0.2.1",
  "description": "eslint flat-config",
  "keywords": [
    "eslint",
    "config",
    "eslint-config",
    "flat-config",
    "module"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uiolee/eslint-config-uiolee.git"
  },
  "license": "MPL-2.0",
  "author": "uiolee (https://github.com/uiolee)",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./configs": "./dist/configs/index.js",
    "./rules": "./dist/rules/index.js",
    "./utils": "./dist/utils/index.js",
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm tsbf",
    "check-types": "tsc --noEmit",
    "esb": "rm -rf dist && node scripts/esb.mjs",
    "inspect": "pnpm dlx @eslint/config-inspector@latest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "tsb": "tsc -b -v",
    "tsbf": "tsc -b -v -f",
    "tsbw": "tsc -b -v -w"
  },
  "dependencies": {
    "@eslint/js": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.42.0"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^22.0.2",
    "@types/node": "^24.3.1",
    "esbuild": "^0.25.9",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.34.0",
    "eslint-config-uiolee": "link:",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@eslint/js": "^9.12.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.8.0"
  },
  "peerDependenciesMeta": {
    "*": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
